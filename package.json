{
  "name": "javascript-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check dev-server lint:watch lint:scss lint:html",
    "dev-server": "webpack-dev-server -d --hot --config webpack.config.js",
    "security-check": "nsp check",
    "accessibility": "node ./buildScripts/accessibility-validation.js",
    "lint:html": "node ./buildScripts/html-validator.js",
    "lint:eslint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint:eslint -- --watch",
    "lint:scss": "scss-lint -c .scss-lint.yml",
    "lint:scss-watch": "scss-lint . -- --watch",
    "test:unit": "cross-env BABEL_ENV=test karma start tests/unit/karma.conf.js --single-run",
    "test:e2e": "nightwatch --config \"tests/e2e/nightwatch.conf.js\"",
    "open:coverage": "start chrome ./tests/unit/coverage/lcov-report/index.html",
    "selenium:start": "selenium-standalone install && selenium-standalone start",
    "watch": "webpack --watch",
    "uglifyjs": "uglifyjs -b beautify=false,max-line-len=120000 dist/main.bundle.js -o dist/main.bundle.min.js --compress --mangle",
    "production": "SET NODE_ENV=production & babel-node buildScripts/build.js & webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KrzysztofLen/javascript-starter-kit.git"
  },
  "author": "Krzysztof Len",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KrzysztofLen/javascript-starter-kit/issues"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "homepage": "https://github.com/KrzysztofLen/javascript-starter-kit#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.18.0",
    "browserify": "^14.4.0",
    "chai": "^4.1.2",
    "chai-dom": "^1.5.0",
    "chalk": "^2.1.0",
    "chromedriver": "^2.33.0",
    "cross-env": "^5.0.5",

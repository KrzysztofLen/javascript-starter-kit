    "css-loader": "^0.28.5",
    "debug": "^2.6.9",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "geckodriver": "^1.9.0",
    "html5-lint": "^0.3.0",
